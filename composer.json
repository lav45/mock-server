{
    "name": "lav45/mock-server",
    "type": "library",
    "description": "HTTP mocking application for testing",
    "keywords": [
        "testing",
        "mock",
        "php",
        "http",
        "mock-server",
        "stubbing"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksey Loban",
            "email": "lav451@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.3",
        "ext-pcntl": "*",
        "amphp/amp": "^v3.0",
        "amphp/log": "^v2.0",
        "amphp/sync": "^v2.1",
        "amphp/http": "^v2.1",
        "amphp/http-client": "^v5.0",
        "amphp/http-server": "^v3.2",
        "amphp/http-server-form-parser": "^v2.0",
        "amphp/byte-stream": "^v2.1",
        "amphp/socket": "^v2.2",
        "nikic/fast-route": "^1.3",
        "monolog/monolog": "^3.5",
        "fakerphp/faker": "^1.23",
        "yiisoft/data": "^1.0",
        "league/uri": "^7.4"
    },
    "require-dev": {
        "maglnet/composer-require-checker": "^4.8",
        "icanhazstring/composer-unused": "^0.8",
        "phpunit/phpunit": "^10.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox",
        "check": "vendor/bin/composer-require-checker check",
        "unused": "vendor/bin/composer-unused"
    },
    "autoload": {
        "psr-4": {
            "lav45\\MockServer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "lav45\\MockServer\\test\\": "test"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "classmap-authoritative": true,
        "optimize-autoloader": true,
        "apcu-autoloader": true,
        "preferred-install": {
            "*": "dist"
        }
    }
}
