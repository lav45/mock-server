{
    "name": "lav45/mock-server",
    "version": "3.2.7",
    "type": "library",
    "description": "HTTP mocking application for testing",
    "keywords": [
        "testing",
        "mock",
        "php",
        "http",
        "mock-server",
        "stubbing"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksey Loban",
            "email": "lav451@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.4",
        "ext-intl": "*",
        "ext-uv": "*",
        "ext-filter": "*",
        "ext-inotify": "*",
        "amphp/amp": "^v3.1",
        "amphp/log": "^v2.0",
        "amphp/sync": "^v2.3",
        "amphp/http": "^v2.1",
        "amphp/http-client": "^v5.3",
        "amphp/http-server": "^v3.4",
        "amphp/http-server-form-parser": "^v2.0",
        "amphp/byte-stream": "^v2.1",
        "amphp/socket": "^v2.3",
        "amphp/file": "^3.2",
        "nikic/fast-route": "^v1.3",
        "monolog/monolog": "^3.9",
        "fakerphp/faker": "^v1.24",
        "yiisoft/data": "^1.0",
        "psr/log": "^3.0",
        "lav45/watcher": "^1.0"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.8",
        "roave/security-advisories": "dev-latest"
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": true,
            "target-directory": "tools",
            "forward-command": true
        }
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    },
    "scripts": {
        "phpunit": "php vendor/bin/phpunit",
        "coverage": "php -d pcov.enabled=1 vendor/bin/phpunit --coverage-html test/coverage",
        "codecov": "php -d pcov.enabled=1 vendor/bin/phpunit --coverage-clover=coverage.xml",
        "check": "php vendor/bin/composer-require-checker check",
        "unused": "php vendor/bin/composer-unused",
        "deptrac": "php vendor/bin/deptrac --cache-file=.cache/.deptrac.cache analyse",
        "code-style": "PHP_CS_FIXER_IGNORE_ENV=1 php vendor/bin/php-cs-fixer fix",
        "test": [
            "@check",
            "@unused",
            "@deptrac",
            "@phpunit"
        ]
    },
    "autoload": {
        "psr-4": {
            "Lav45\\MockServer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lav45\\MockServer\\Test\\": "test"
        }
    },
    "minimum-stability": "stable"
}
